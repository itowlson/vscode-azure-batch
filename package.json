{
  "name": "merlin-the-happy-extension",
  "displayName": "Merlin the Happy Extension",
  "description": "See the little goblin, see his little teeth",
  "version": "0.0.1",
  "publisher": "itowlson",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:merlin.invokeAllThePythons"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "merlin.invokeAllThePythons",
        "title": "Invoke All the Pythons",
        "category": "Merlin"
      },
      {
        "command": "merlin.itCreatesUsTheJobOrUsKillsItTheKitten",
        "title": "Create Job",
        "category": "Azure Batch"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/shelljs": "^0.7.1",
    "@types/websocket": "0.0.33",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "websocket": "^1.0.24",
    "shelljs": "^0.7.7"
  }
}
